{
  "compilerOptions": {
    "noImplicitAny": false,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": false,
    "resolveJsonModule": true,
    "typeRoots": ["./types", "./node_modules/@types"],
    "target": "es5",
    "lib": ["es2017", "dom"],
    "sourceMap": true,
    "outDir": "dist/",
    "declaration": true,
    "declarationDir": "dist/types",
    "esModuleInterop": true
  },
  "include": ["src"]
}
